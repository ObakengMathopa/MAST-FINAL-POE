<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FINALPOE">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="1346dp"
        android:layout_height="1494dp"
        app:srcCompat="@drawable/CYBER"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-664dp"
        tools:layout_editor_absoluteY="-486dp" />

    <EditText
        android:id="@+id/editTextTextMultiLine"
        android:layout_width="238dp"
        android:layout_height="72dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="577dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/NEON"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="145dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextTextMultiLine"
        android:layout_width="238dp"
        android:layout_height="72dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        tools:ignore="DuplicateIds,MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="505dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter Name"
        android:textColor="#F80054"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="544dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter Price"
        android:textColor="#FF0057"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="615dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="208dp"
        android:layout_height="225dp"
        app:srcCompat="@drawable/INSERT"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="203dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Insert Image"
        android:textColor="#FF0057"
        tools:layout_editor_absoluteX="165dp"
        tools:layout_editor_absoluteY="409dp"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="155dp"
        android:layout_height="62dp"
        app:srcCompat="@drawable/bUTTON"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="649dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Save"
        android:textColor="#FF0057"
        tools:layout_editor_absoluteX="190dp"
        tools:layout_editor_absoluteY="664dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>